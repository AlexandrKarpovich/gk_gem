#carouselExampleFade.carousel.slide.carousel-fade(data-ride='carousel')
    .carousel-inner
        .carousel-item.active
            img.d-block.w-100(src='img/home-cura_casa-2019.jpg', alt='home-cura_casa')
        .carousel-item
            img.d-block.w-100(src='img/Winnis_10Anni_Homepage_CTA.jpg', alt='Winnis_10Anni_Homepage_CTA')
        .carousel-item
            img.d-block.w-100(src='img/Winnis_Risparmio_Homepage_CTA-1.jpg', alt='Winnis_Risparmio_Homepage_CTA')
    a.carousel-control-prev(href='#carouselExampleFade', role='button', data-slide='prev')
        span.carousel-control-prev-icon(aria-hidden='true')
        span.sr-only Previous
    a.carousel-control-next(href='#carouselExampleFade', role='button', data-slide='next')
        span.carousel-control-next-icon(aria-hidden='true')
        span.sr-only Next


